# Basic binds
super + Return
	$TERMINAL
super + shift + Return
	tmuxmanager
super + shift + s
	sshmanager
super + d
	dmenu_run -l 20
super + r
	$TERMINAL -e ranger
super + q
	$BROWSER
super + c
  code-oss; sleep 1;

# Function keys
# Show readme
super + F1
  playerctl play
super + F2
  playerctl pause
super + F3
  playerctl previous
super + F4
  playerctl next
super + F5
  mymaster
